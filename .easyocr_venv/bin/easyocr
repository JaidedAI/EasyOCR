#!/home/ystoll/PixelForest/easyocr/.easyocr_venv/bin/python3.8
# EASY-INSTALL-ENTRY-SCRIPT: 'easyocr','console_scripts','easyocr'
__requires__ = 'easyocr'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('easyocr', 'console_scripts', 'easyocr')()
    )
